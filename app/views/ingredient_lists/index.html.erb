<h1>Listing ingredient_lists</h1>

<table>
  <tr>
    <th>Ingredient</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ingredient_lists.each do |ingredient_list| %>
  <tr>
    <td><%= ingredient_list.ingredient_id %></td>
    <td><%= link_to 'Show', ingredient_list %></td>
    <td><%= link_to 'Edit', edit_ingredient_list_path(ingredient_list) %></td>
    <td><%= link_to 'Destroy', ingredient_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

